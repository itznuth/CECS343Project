
class Tenant:
  """Represents Tenant class.

  Attributes
    space_number (int): Indicates the store number occupied by the tenant.
    lease_start_date (str): Represents the start date of the tenant's lease.
    lease_end_date(str): Represents the end date of the tenantâ€™s name.

  Methods:
    set_store_number(number: int) -> None: Assigns the store number to the tenant.
    set_lease_start_date(date: str) -> None: Sets the start date of the tenant's lease.
    get_lease_start_date() -> str: Retrieves the start date of the tenant's lease.
    set_lease_end_date(date: str) -> None: Sets the end date of the tenantâ€™s lease.
    get_lease_end_date() -> str: Retrieves the end date of the tenantâ€™s.
  """
  pass


'''This class will represent the tenant in our mall management system'''
class Tenant:
    def __init__(self,name:str,space_number:int,lease_start_date:str,lease_end_date:str,contact_information:str):
        '''This initializes the tenant object with their basic details and contact information'''
        set_name = self.name
        set_store_number = self.space_number
        set_lease_start_date = self.lease_start_date
        set_lease_end_date = self.lease_end_date
        set_contact_information = self.contact_information


  
    def set_name(self,name:str) -> None :
        set_name = self.name

    def set_store_number(self,store_number:int) -> None:
        set_store_number = self.space_number

    def set_lease_start_date(self,lease_start_date:str) -> None:
        set_lease_start_date = self.lease_start_date

    def set_lease_end_date(self,lease_end_date:str) -> None:
        set_lease_end_date = self.lease_end_date

    def set_contact_information(self,contact_information:str) -> None:
        set_contact_information = self.contact_information



    def get_lease_start_date(self) -> str:
        return self.set_lease_start_date

    def get_lease_end_date(self) -> str:
        return self.set_lease_end_date


